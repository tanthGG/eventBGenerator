context Refine_Machine_C3
sets
  PKT
  TYPE

constants
  safetyThreshold
  ND
  Dests
  initialSrcAddr
  finalDestAddr
  type
  DATA

axioms
  @ax01 safetyThreshold ∈ ℤ
  @ax02 ND ⊆ ℕ
  @ax03 finite(ND)
  @ax04 Dests ⊆ ND
  @ax05 finite(PKT)
  @ax06 initialSrcAddr ∈ PKT → ND
  @ax07 finalDestAddr ∈ ℙ(PKT × ℤ)
  @ax08 ND ⊆ ℕ ∧ finite(ND)
  @ax09 DATA ∈ TYPE
  @ax10 type ∈ PKT → TYPE

end
